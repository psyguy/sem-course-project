  month = oct,
  year = {1994},
  pages = {87--102},
  file = {C:\\Users\\mhsci\\Zotero\\storage\\UER63XXQ\\Willshaw, Dennett, Partridge, 1994, Non-symbolic approaches to artificial intelligence and the mind and Discussion.pdf}
}

