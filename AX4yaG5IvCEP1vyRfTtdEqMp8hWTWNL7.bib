@book{hurford_textlessitextgreaterneuroscience_2007,
  title = {\textbackslash{}textlessi\textbackslash{{textgreaterNeuroscience}} and {{Philosophy}}: {{Brain}}, {{Mind}}, and {{Language}}\textbackslash{}textless/I\textbackslash{}textgreater . \textbackslash{}textlessi\textbackslash{{textgreaterBy}}\textbackslash{}textless/I\textbackslash{}textgreater {{Maxwell Bennett}} \textbackslash{}textlessi\textbackslash{}textgreater,\textbackslash{}textless/I\textbackslash{}textgreater , {{Daniel Dennett}} \textbackslash{}textlessi\textbackslash{}textgreater,\textbackslash{}textless/I\textbackslash{}textgreater , {{Peter Hacker}} \textbackslash{}textlessi\textbackslash{}textgreater, And\textbackslash{}textless/I\textbackslash{}textgreater , {{John Searle}} \textbackslash{}textlessi\textbackslash{}textgreater; {{Introduction}} and {{Conclusion}} by {{Daniel Robinson}}. {{New York}}: {{Columbia University P}}},
