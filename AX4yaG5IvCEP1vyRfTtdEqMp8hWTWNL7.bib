@book{pedersen_significance_2016,
  address = {{Leuven}},
  title = {The {{Significance}} of the {{Body}} - {{Artificial Intellgeince}} and {{Embodied Cognition}}},
  abstract = {Artificial intelligence is a field with a diverse set of goals, and approaches to artificial intelligence. One approach to artificial intelligence takes the theoretical idea of embodiment, found predominately in phenomenology, into account in an attempt to, in the strongest sense of artificial intelligence, ``create persons''. Obviously, this approach faces a number of issues. This thesis delivers a critique of the lack of consideration of the specificity of the body found in robotics combined with artificial intelligence in the strong sense. Although this account takes the body into consideration, it fails to realize that the body plays a constitutive role in cognition. Moreover, the very aim of the project of `strong' artificial intelligence, namely to `create persons', is highly problematic. If persons are indeed created, it seems plausible to hold, and arguments are presented to the fact that these robotic persons would have to be granted inviolable rights and citizenship rights, on behalf of their personhood. As a consequence, this thesis proposes to abolish strong artificial intelligence in favour of what could be termed instrumental artificial intelligence. Instrumental artificial intelligence gives direct consideration to the specifics of embodiment and to its implications for cognition. By implementing instrumental artificial intelligence creating persons whom would have to be granted inviolable rights and even citizenship rights is avoidable. Instead, robotic agents with a high degree of artificial intelligence can be created without facing issues of reintroducing slavery by implementing instrumental artificial intelligence. Instrumental artificial intelligence provides a platform for addressing two traditionally distinct sides of artificial intelligence at the same time. On the one hand, it highlights the constitutive significance of embodiment for cognition. On the other hand, it draws out ethical and political considerations of embodied cognition. This also means, I argue, that the specificity of the body has much wider consequences than has been traditionally, and often is at present, assumed.},
  publisher = {{KU Leuven. Hoger Instituut voor Wijsbegeerte}},
  author = {Pedersen, Thomas Rammekjaer Rud},
  year = {2016}
}

@book{chalmers_conscious_1996,
  title = {The {{Conscious Mind}}: {{In Search}} of a {{Fundamental Theory}}},
  isbn = {978-0-19-802653-2},
  shorttitle = {The {{Conscious Mind}}},
  abstract = {What is consciousness? How do physical processes in the brain give rise to the self-aware mind and to feelings as profoundly varied as love or hate, aesthetic pleasure or spiritual yearning? These questions today are among the most hotly debated issues among scientists and philosophers, and we have seen in recent years superb volumes by such eminent figures as Francis Crick, Daniel C. Dennett, Gerald Edelman, and Roger Penrose, all firing volleys in what has come to be called the consciousness wars. Now, in The Conscious Mind, philosopher David J. Chalmers offers a cogent analysis of this heated debate as he unveils a major new theory of consciousness, one that rejects the prevailing reductionist trend of science, while offering provocative insights into the relationship between mind and brain. Writing in a rigorous, thought-provoking style, the author takes us on a far-reaching tour through the philosophical ramifications of consciousness. Chalmers convincingly reveals how contemporary cognitive science and neurobiology have failed to explain how and why mental events emerge from physiological occurrences in the brain. He proposes instead that conscious experience must be understood in an entirely new light--as an irreducible entity (similar to such physical properties as time, mass, and space) that exists at a fundamental level and cannot be understood as the sum of its parts. And after suggesting some intriguing possibilities about the structure and laws of conscious experience, he details how his unique reinterpretation of the mind could be the focus of a new science. Throughout the book, Chalmers provides fascinating thought experiments that trenchantly illustrate his ideas. For example, in exploring the notion that consciousness could be experienced by machines as well as humans, Chalmers asks us to imagine a thinking brain in which neurons are slowly replaced by silicon chips that precisely duplicate their functions--as the neurons are replaced, will consciousness gradually fade away? The book also features thoughtful discussions of how the author's theories might be practically applied to subjects as diverse as artificial intelligence and the interpretation of quantum mechanics. All of us have pondered the nature and meaning of consciousness. Engaging and penetrating, The Conscious Mind adds a fresh new perspective to the subject that is sure to spark debate about our understanding of the mind for years to come.},
  language = {en},
  publisher = {{Oxford University Press}},
  author = {Chalmers, David J.},
  month = may,
  year = {1996},
  keywords = {Psychology / Cognitive Psychology \& Cognition,Philosophy / Mind \& Body,Medical / Neurology}
}

